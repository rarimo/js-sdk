{
  "name": "@rarimo/shared-zkp-iden3",
  "version": "2.0.0-rc.19",
  "description": "Internal tools that other Rarimo packages use for identity functionality.",
  "repository": {
    "type": "git",
    "url": "https://github.com/rarimo/js-sdk.git",
    "directory": "packages/shared-zkp-iden3"
  },
  "license": "MIT",
  "sideEffects": false,
  "typesVersions": {
    ">=4.2": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "node": "./dist/cjs/index.js",
  "unpkg": "./dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./assets": {
      "default": "./dist/assets/**/*"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:cjs && yarn build:esm && node ./postbuild.js",
    "build:types": "tsc -p tsconfig.build.json --outDir ./dist/types --declaration --emitDeclarationOnly",
    "build:cjs": "npx swc src -d ./dist/cjs --config-file ../../.swcrc -C module.type=commonjs",
    "build:esm": "npx swc src -d ./dist/esm --config-file ../../.swcrc -C module.type=es6 isModule=true",
    "clean": "rm -rf dist",
    "test": "yarn jest --verbose",
    "typecheck": "tsc --noEmit",
    "generate-ether-types": "typechain --target=ethers-v5 'abis/**/*.json' --out-dir src/types/contracts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@swc/jest": "^0.2.26",
    "@typechain/ethers-v5": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "tsc-alias": "^1.8.2",
    "typechain": "^8.2.0"
  },
  "dependencies": {
    "@distributedlab/fetcher": "^1.0.0-rc.6",
    "@rarimo/client": "workspace:^",
    "tslib": "^2.5.0"
  },
  "typedocOptions": {
    "entryPoints": [
      "src/index.ts"
    ]
  }
}
