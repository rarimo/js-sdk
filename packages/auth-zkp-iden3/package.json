{
  "name": "@rarimo/auth-zkp-iden3",
  "version": "2.0.0-rc.2",
  "description": "Generating identity for iden3 protocol",
  "repository": {
    "type": "git",
    "url": "https://github.com/rarimo/js-sdk.git",
    "directory": "packages/auth-zkp-iden3"
  },
  "license": "MIT",
  "sideEffects": false,
  "typesVersions": {
    ">=4.2": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "node": "./dist/cjs/index.js",
  "unpkg": "./dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./assets": {
      "default": "./dist/assets/**/*"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:cjs && yarn build:esm && node ./postbuild.js",
    "build:types": "tsc -p tsconfig.build.json --outDir ./dist/types --declaration --emitDeclarationOnly",
    "build:cjs": "npx swc src -d ./dist/cjs --config-file ../../.swcrc -C module.type=commonjs",
    "build:esm": "rollup -c --bundleConfigAsCjs",
    "clean": "rm -rf dist",
    "test": "yarn jest --verbose",
    "typecheck": "tsc --noEmit",
    "generate-ether-types": "typechain --target=ethers-v5 './abi/**/*.json' --out-dir src/types/contracts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@swc/jest": "^0.2.26",
    "@typechain/ethers-v5": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "tsc-alias": "^1.8.2",
    "typechain": "^8.2.0"
  },
  "dependencies": {
    "@distributedlab/fetcher": "^0.2.0-rc.20",
    "@iden3/js-crypto": "^1.0.0-beta.1",
    "@iden3/js-iden3-core": "^1.0.0-beta.2",
    "@iden3/js-jwz": "^1.0.0-beta.2",
    "@iden3/js-merkletree": "^1.0.0-beta.4",
    "@rarimo/identity-gen-iden3": "workspace:^",
    "@rarimo/shared": "workspace:^",
    "tslib": "^2.5.0"
  }
}
