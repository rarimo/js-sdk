{
  "name": "@rarimo/zkp-gen-iden3",
  "version": "2.0.0-rc.20",
  "description": "Tools that generate zero-knowledge proofs for the Iden3 protocol.",
  "repository": {
    "type": "git",
    "url": "https://github.com/rarimo/js-sdk.git",
    "directory": "packages/zkp-gen-iden3"
  },
  "license": "MIT",
  "sideEffects": false,
  "typesVersions": {
    ">=4.2": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "node": "./dist/cjs/index.js",
  "unpkg": "./dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./assets": {
      "default": "./dist/assets/**/*"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:cjs && yarn build:esm && node ./postbuild.js",
    "build:types": "tsc -p tsconfig.build.json --outDir ./dist/types --declaration --emitDeclarationOnly",
    "build:cjs": "npx swc src -d ./dist/cjs --config-file ../../.swcrc -C module.type=commonjs",
    "build:esm": "npx swc src -d ./dist/esm --config-file ../../.swcrc -C module.type=es6 isModule=true",
    "clean": "rm -rf dist",
    "test": "yarn jest --verbose",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@iden3/js-crypto": "^1.0.0-beta.1",
    "@iden3/js-iden3-core": "1.0.0-beta.2",
    "@iden3/js-jsonld-merklization": "^1.0.0-beta.14",
    "@iden3/js-jwz": "^1.0.0-beta.2",
    "@iden3/js-merkletree": "^1.0.0-beta.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "tsc-alias": "^1.8.2"
  },
  "dependencies": {
    "@distributedlab/fetcher": "^1.0.0-rc.6",
    "@distributedlab/tools": "^1.0.0-rc.6",
    "@rarimo/auth-zkp-iden3": "workspace:^",
    "@rarimo/client": "workspace:^",
    "@rarimo/identity-gen-iden3": "workspace:^",
    "@rarimo/shared": "workspace:^",
    "@rarimo/shared-zkp-iden3": "workspace:^",
    "tslib": "^2.5.0"
  },
  "peerDependencies": {
    "@iden3/js-crypto": "^1.0.0-beta.1",
    "@iden3/js-iden3-core": "1.0.0-beta.2",
    "@iden3/js-jsonld-merklization": "^1.0.0-beta.14",
    "@iden3/js-jwz": "^1.0.0-beta.2",
    "@iden3/js-merkletree": "^1.0.0-beta.4"
  },
  "typedocOptions": {
    "entryPoints": [
      "src/index.ts"
    ]
  }
}
