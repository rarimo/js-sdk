{
  "name": "@rarimo/react-nft-checkout",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rarimo/js-sdk/tree/main/packages/react-nft-checkout"
  },
  "main": "./lib/cjs/index.js",
  "unpkg": "./lib/rarimo-react-nft-checkout.min.js",
  "module": "./lib/esm/index.js",
  "browser": "./lib/rarimo-react-nft-checkout.min.js",
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:cjs && yarn build:esm",
    "build:cjs": "babel src --out-dir lib/cjs --extensions \".ts,.tsx\" --config-file ./babel.config.node.json",
    "build:esm": "babel src --out-dir lib/esm --extensions \".ts,.tsx\" --config-file ./babel.config.json",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.8",
    "@rarimo/nft-checkout": "workspace:*",
    "iconoir-react": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.6.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "happy-dom": "^8.1.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "typechain": "^8.1.0",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
