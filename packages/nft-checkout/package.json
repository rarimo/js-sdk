{
  "name": "@rarimo/nft-checkout",
  "version": "1.0.0",
  "description": "TypeScript-based source development script for Rarimo Protocol",
  "repository": {
    "type": "git",
    "url": "https://github.com/rarimo/js-sdk/tree/main/packages/nft-checkout"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "unpkg": "./lib/index.js",
  "module": "./lib/esm/index.js",
  "browser": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:cjs && yarn build:esm && yarn build:browser",
    "build:browser": "yarn node ../../esbuild.js",
    "build:cjs": "babel src --out-dir lib/cjs --extensions \".ts,.tsx\" --config-file ./babel.config.node.json",
    "build:esm": "babel src --out-dir lib/esm --extensions \".ts,.tsx\" --config-file ./babel.config.json",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "clean": "rm -rf lib",
    "test": "yarn jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@distributedlab/utils": "^3.1.0",
    "@rarimo/provider": "workspace:*",
    "@traderjoe-xyz/sdk": "2.0.7",
    "@uniswap/sdk-core": "3.1.0",
    "@uniswap/smart-order-router": "^2.10.0",
    "@uniswap/token-lists": "1.0.0-beta.30",
    "@uniswap/v3-sdk": "^3.9.0",
    "axios": "^1.3.1",
    "ethereum-erc20-token-balances-multicall": "^1.0.2",
    "ethers": "^5.7.2",
    "jsbi": "^3.2.5",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "@rarimo/nft-checkout"
  }
}
