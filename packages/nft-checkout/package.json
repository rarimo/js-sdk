{
  "name": "@rarimo/nft-checkout",
  "description": "TypeScript-based source development script for Rarimo Protocol",
  "version": "1.0.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "browser": "./lib/nft-checkout.min.js",
  "unpkg": "./lib/nft-checkout.min.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "@rarimo/nft-checkout"
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/* --ext .js,.jsx,.ts,.tsx --fix",
    "test": "yarn jest",
    "type-check": "tsc --noEmit",
    "build": "yarn clean && yarn build:types && yarn build:cjs && yarn build:esm",
    "build:cjs": "babel src --out-dir lib/cjs --extensions \".ts,.tsx\" --config-file ./babel.config.node.json",
    "build:esm": "babel src --out-dir lib/esm --extensions \".ts,.tsx\" --config-file ./babel.config.json",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "clean": "rm -rf lib"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "babel-plugin-root-import": "^6.6.0",
    "jest": "^29.4.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@traderjoe-xyz/sdk": "2.0.7",
    "@uniswap/sdk-core": "3.1.0",
    "@uniswap/smart-order-router": "2.10.0",
    "@uniswap/token-lists": "1.0.0-beta.30",
    "@uniswap/v3-sdk": "^3.9.0",
    "axios": "^1.3.1",
    "ethereum-erc20-token-balances-multicall": "^1.0.2",
    "ethers": "^5.7.2",
    "http-status-codes": "^2.2.0",
    "jsbi": "3.2.5",
    "jsona": "^1.11.0",
    "lodash": "^4.17.21"
  }
}
